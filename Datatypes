code 2:
public class StringExample {
    public static void main(String[] args) {
        String name = "Manoj";
        System.out.println("Hello, " + name.toUpperCase());
    }
}
---------------------------------------------------------------------------------------
code 3:
class Car {
    String model;
    int year;
}
public class Main {
    public static void main(String[] args) {
        Car myCar = new Car();
        myCar.model = "Toyota";
        myCar.year = 2020;
        System.out.println("Car: " + myCar.model + " (" + myCar.year + ")");
    }
}
----------------------------------------------------------------------------------------------
code 4:
public class CastingExample {
    public static void main(String[] args) {
        int a = 10;
        double b = a; 
        System.out.println("Double value: " + b);
        double c = 10.99;
        int d = (int) c; 
        System.out.println("Int value: " + d);
    }
}
----------------------------------------------------------
code 5:
public class ByteExample {
    public static void main(String[] args) {
        byte age = 18;
        byte pencils = 100;
        System.out.println("Age: " + age);
        System.out.println("Pencils in box: " + pencils);
    }
}
